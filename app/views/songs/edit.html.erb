<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <p> <%= f.label :title %>
  <%= f.text_field :title %>
  </p>

  <p>  <%= f.label :artist_name %>
    <%= f.text_field :artist_name %> </p>


  <p> <%= f.label :genre_id%>
    <%= f.collection_select :genre_id, Genre.all, :id, :name, prompt:true %> </p>

  <%= f.submit %>
<% end %>

<%= button_to "delete", @song, {method: :delete} %>
